            arr.sort(function (a, b) { return a - b; })
            var v = [];
            v.push(arr[0]);
            for (let i = 1; i < n; i++) { //remove duplicate elements //or use include method
                if (arr[i] != arr[i - 1])
                    v.push(arr[i]);
            }
            console.log(v) 
            for (let i = 0; i < v.length; i++) { //main logic to find longest length
                if (v[i] - v[i - 1] == 1){
                    count++;
                    console.log(v[i])  
                    }                
            }
            return count;
        }
        let arr = [1, 9, 3, 10, 4, 20, 2]; 
