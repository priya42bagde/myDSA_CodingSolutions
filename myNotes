function rowWithMaxOne(arr, n, m){
var ansrow = -1;
var j = m-1;
for(var i=0; i<n && j>=0; i++){
  while(j>=0){
    if(arr[i][j]==1){
      j--;
      ansrow = i;
    }
    else{
      break;
    }
  }
}
return ansrow;
}
