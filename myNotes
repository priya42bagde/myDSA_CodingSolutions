function printDups(str){
    var count = {};
    for (var i = 0; i < str.length; i++) {
        count[str[i]] = 0; } console.log(count)
    for (var i = 0; i < str.length; i++) {
        count[str[i]]++;   } console.log(count)
    for (var it in count) {
        if (count[it] > 1) document.write(it + ", count = " + count[it] + "<br>");
    }
}
var str = "test string";
printDups(str);
