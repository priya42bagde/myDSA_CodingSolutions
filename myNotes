function getPairsCount(arr, n, sum)
{
    let count = 0; // Initialize result
    for (let i = 0; i < n; i++)
        for (let j = i + 1; j < n; j++)
            if (arr[i] + arr[j] == sum){
                count++;
                console.log(arr[i], arr[j])
                }

    return count;
}
