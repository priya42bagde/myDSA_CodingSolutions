Reverse String:-
function reverseString(s){
  var i = 0, j = s.length -1;
        while(i < j){ //0<4 //1<3
            var ch = s[i];
            s[i] = s[j];
            s[j] = ch;
            i++;j--;
        }
    return s;    
}
console.log(reverseString(["h","e","l","l","o"]))   
