     function printSorted(Mat,N)
    {
       var a=new Array(N * N);
       var k=0;
       for(var i=0;i<N;i++)
       {
           for(var j=0;j<N;j++)
           {
               a[k]=Mat[i][j];
               k++;
           }
       }
       a.sort();
       var x=0;
       for(var i=0;i<N;i++)
       {
           for(var j=0;j<N;j++)
           {
               Mat[i][j]=a[x];
               x++;
           }
       }
       return Mat;
   }
let Mat=[[10, 20, 30, 40],[15, 25, 35, 45],
             [27, 29, 37, 48],[32, 33, 39, 50]];
